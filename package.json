{
  "name": "squidlet-lib",
  "version": "0.1.10",
  "description": "Common lib for squidlet projects",
  "author": "Ivan Kozyrin <ipkozyrin@gmail.com>",
  "repository": "git@github.com:bozonx/squidlet-lib.git",
  "type": "module",
  "main": "./distr/lib/index.js",
  "module": "./distr/lib/index.js",
  "types": "./distr/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./distr/types/index.d.ts",
      "import": "./distr/lib/index.js",
      "require": "./distr/lib/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "distr"
  ],
  "publishConfig": {
    "main": "./distr/lib/index.js",
    "module": "./distr/lib/index.js",
    "types": "./distr/types/index.d.ts",
    "exports": {
      ".": {
        "types": "./distr/types/index.d.ts",
        "import": "./distr/lib/index.js",
        "require": "./distr/lib/index.cjs"
      },
      "./package.json": "./package.json"
    }
  },
  "license": "MIT",
  "scripts": {
    "build": "tsc && npm run build:cjs",
    "build:cjs": "tsc --project tsconfig.cjs.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "coverage": "jest --coverage",
    "lint": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^24.1.0",
    "jest": "^29.7.0",
    "prettier": "^3.6.2",
    "prettier-plugin-jsdoc": "^1.3.3",
    "ts-jest": "^29.4.0",
    "typescript": "^5.8.3"
  }
}
